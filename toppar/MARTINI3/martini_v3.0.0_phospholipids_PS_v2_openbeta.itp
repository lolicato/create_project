   2  3 	b_PO4_GL_def
   2  4 	b_PO4_GL_def_long
   3  4 	b_GL_GL_glyc
   3  5 	b_GL_SC1_glyc
   5  6 	b_SC1_C1_end
   4  7 	b_GL_SC1_glyc
   7  8 	b_SC1_C1_end

[angles]
